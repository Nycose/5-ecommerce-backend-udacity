{
    "name": "storefront",
    "version": "0.1.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "npm run dev-up && nodemon src/server.ts",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
        "build": "npx tsc",
        "lint": "eslint 'src/**/*.ts'",
        "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "jasmine": "jasmine",
        "test": "npm run build && npm run test-up && cross-env ENVI=test jasmine && npm run test-down",
        "dev-up": "db-migrate up -m ./migrations   --config ./database.json",
        "dev-down": "db-migrate reset -m ./migrations  --config ./database.json",
        "test-up": "db-migrate up -e test -m ./migrations   --config ./database.json",
        "test-down": "db-migrate reset  -e test -m ./migrations  --config ./database.json"
    },
    "author": "Udacity",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.5.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jasmine": "^3.10.2",
        "@types/jest": "^27.0.2",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/node": "^16.11.6",
        "@types/pg": "^7.14.7",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^5.1.1",
        "jasmine": "^3.10.0",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.14",
        "prettier": "^2.4.1",
        "supertest": "^6.1.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.1.3"
    }
}
